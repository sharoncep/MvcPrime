        <li><a href="@Url.RouteUrl(StaticClass.RouteValues("SpecialtyBlock"))"  rel="menu_tile" class="menu-specialty-mgr" onclick="javascript:showDivPageLoading();">@Resources.ClaimatePrimeWebResx.Specialty</a></li>
