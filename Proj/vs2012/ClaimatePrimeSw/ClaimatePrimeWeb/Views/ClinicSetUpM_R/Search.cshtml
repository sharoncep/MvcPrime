@{
    Layout = "~/Views/Shared/MasterPages/Home.cshtml";
}
@using ClaimatePrimeControllers.SecuredFolder.StaticClasses;
@using ClaimatePrimeModels.Models;
@using ClaimatePrimeControllers.SecuredFolder.SessionClasses;

<div class="dv-page-header1">
    <ul class="ul-header">


        <li><a href="@Url.RouteUrl(StaticClass.RouteValues("ClinicViewM", "Search", 1, 0))" title="@Resources.ClaimatePrimeWebResx.Back" class="back-button" onclick="javascript:showDivPageLoading();"></a></li>
        <li>@Resources.ClaimatePrimeWebResx.ClinicSetup</li>
    </ul>
</div>
@Html.Partial("UserControls/PageHeaderSub")
<div class="dv-block">
    <ul class="ul-menu">
        <li><a href="@Url.RouteUrl(StaticClass.RouteValues("ClinicSetUpM", "Save"))" rel="menu_tile" onclick="javascript:showDivPageLoading();" class="menu-edit-ba">@Resources.ClaimatePrimeWebResx.Edit</a></li>
        <li><a href="@Url.RouteUrl(StaticClass.RouteValues("ProviderM"))" rel="menu_tile" onclick="javascript:showDivPageLoading();" class="menu-provider-mgr">@Resources.ClaimatePrimeWebResx.Provider</a></li>
        <li><a  href="@Url.RouteUrl(StaticClass.RouteValues("CaseComplex"))" rel="menu_tile" onclick="javascript:showDivPageLoading();"class="menu-case-complexity-mgr">@Resources.ClaimatePrimeWebResx.CaseComplexity</a></li>
        <li><a href="@Url.RouteUrl(StaticClass.RouteValues("CaseAgentM"))" rel="menu_tile" onclick="javascript:showDivPageLoading();" class="menu-case-agent-mgr">@Resources.ClaimatePrimeWebResx.CaseAgent</a></li>
        <li><a  href="@Url.RouteUrl(StaticClass.RouteValues("CaseReopen"))" rel="menu_tile" onclick="javascript:showDivPageLoading();" class="menu-case-reopen-mgr">@Resources.ClaimatePrimeWebResx.CaseReOpen</a></li>
        <li><a href="@Url.RouteUrl(StaticClass.RouteValues("CaseReclose"))" rel="menu_tile" onclick="javascript:showDivPageLoading();" class="menu-case-reclose-mgr">@Resources.ClaimatePrimeWebResx.CaseReClose</a></li>

    </ul>
</div>

<div style="display: none;">
    @*Error Or Success Message Displaying*@
    @if ((ViewBag.ErrMsg > 0) || (ViewBag.SuccMsg > 0))
    {
        if (ViewBag.ErrMsg > 0)
        {
            if (ViewBag.ErrMsg == 1)
            {
        <input type="text" name="txtErrMsg"  value="@Resources.ClaimatePrimeWebResx.SaveErrorSucc" />
            }
            else
            {
        <input type="text" name="txtErrMsg"  value="@Resources.ClaimatePrimeWebResx.SaveErrorSucc" />
            }
        }
        else
        {
            if (ViewBag.SuccMsg == 1)
            {
        <input type="text" name="txtSuccMsg"  value="@Resources.ClaimatePrimeWebResx.SaveSucc" />
            }
            else
            {
        <input type="text" name="txtSuccMsg"  value="@Resources.ClaimatePrimeWebResx.SaveErrorSucc" />
            }
        }
    }
</div>
