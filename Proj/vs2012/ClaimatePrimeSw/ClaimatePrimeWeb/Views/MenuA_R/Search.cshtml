@{
    Layout = "~/Views/Shared/MasterPages/Home.cshtml";
}
@using ClaimatePrimeControllers.SecuredFolder.StaticClasses;
@using ClaimatePrimeModels.Models;
@using ClaimatePrimeControllers.SecuredFolder.SessionClasses;

<div class="dv-page-header1">
    <ul class="ul-header">
        <li><a href="@Url.RouteUrl(StaticClass.RouteValues("RoleSelection", "Search"))" title="@Resources.ClaimatePrimeWebResx.Back" class="back-button" onclick="javascript:showDivPageLoading();"></a></li>
        <li>@Resources.ClaimatePrimeWebResx.Administration</li>
    </ul>
</div>
@Html.Partial("UserControls/PageHeaderSub")
<div class="dv-block">
    <ul class="ul-menu">
        <li><a href="@Url.RouteUrl(StaticClass.RouteValues("MenuAddress", "Search"))" rel="menu_tile" class="menu-address-mgr" onclick="javascript:showDivPageLoading();">@Resources.ClaimatePrimeWebResx.Address</a></li>
        <li><a href="@Url.RouteUrl(StaticClass.RouteValues("MenuBillInfoA"))"  rel="menu_tile" class="menu-billing-mgr" onclick="javascript:showDivPageLoading();">@Resources.ClaimatePrimeWebResx.BillingInfo</a></li>
        <li><a href="@Url.RouteUrl(StaticClass.RouteValues("MenuConfig", "Search"))" rel="menu_tile" class="menu-config" onclick="javascript:showDivPageLoading();">@Resources.ClaimatePrimeWebResx.Configuration</a></li>
        <li><a href="@Url.RouteUrl(StaticClass.RouteValues("MenuUserA"))"  rel="menu_tile" class="menu-user-info" onclick="javascript:showDivPageLoading();">@Resources.ClaimatePrimeWebResx.UserInfo</a></li>
    </ul>
</div>
