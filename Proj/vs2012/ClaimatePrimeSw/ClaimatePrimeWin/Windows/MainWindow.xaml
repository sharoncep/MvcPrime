<base:BaseWpf x:Class="ClaimatePrimeWin.Windows.MainWindow" x:Name="cphmoMainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:base="clr-namespace:ClaimatePrimeWin.SecuredFolder.BaseWpfs"
        Style="{StaticResource FormStyle}" WindowStartupLocation="CenterScreen">
    <Border Style="{StaticResource BorderStyle}">
        <Label Name="lblPageHeader" Content="{Binding Title, ElementName=cphmoMainWindow}" ToolTip="{Binding Title, ElementName=cphmoMainWindow}" Style="{StaticResource LabelStyle}" Foreground="#62327d" />
    </Border>
</base:BaseWpf>
